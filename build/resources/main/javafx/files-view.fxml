<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.TextArea?>
<VBox xmlns:fx="http://javafx.com/fxml"
      fx:controller="controllers.FileController"
      alignment="CENTER" spacing="20.0">

    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Button text="Return to dashboard" onAction="#handleSwitchToDashboard"/>
    <Button text="Upload image" onAction="#handleUploadButtonEvent"/>

    <TextArea fx:id="asciiOutputArea"/>

</VBox>
